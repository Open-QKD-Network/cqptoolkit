+    . auto/feature
+fi
+
+if [ $ngx_found = no ]; then
+
+    ngx_feature="cqptoolkit in release"
+    ngx_feature_path="/home/rc15345/code/trunk/src/CQPToolkit /home/rc15345/code/build-trunk-Desktop-Release/src"
