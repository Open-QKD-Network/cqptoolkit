+    if [ $NGX_RPATH = YES ]; then
+        ngx_feature_libs="-R/home/rc15345/code/build-trunk-Desktop-Release/src/CQPToolkit -L/home/rc15345/code/build-trunk-Desktop-Release/src/CQPToolkit -l:libCQPToolkit-x86_64.a -lcurl  -lstdc++ -ldl"
+    else
+        ngx_feature_libs="-L/home/rc15345/code/build-trunk-Desktop-Release/src/CQPToolkit -l:libCQPToolkit-x86_64.a -lcurl  -lstdc++ -ldl"
+    fi
+
+    . auto/feature
+fi
+
+if [ $ngx_found = no ]; then
+
+    ngx_feature="cqptoolkit in dev"
+    ngx_feature_path="/home/rc15345/code/CQPToolkit/src/CQPToolkit /home/rc15345/code/CQPToolkit/build/gcc/src"
+
+    if [ $NGX_RPATH = YES ]; then
+        ngx_feature_libs="-R/home/rc15345/code/CQPToolkit/build/gcc/src/CQPToolkit -L/home/rc15345/code/CQPToolkit/build/gcc/src/CQPToolkit -l:libCQPToolkit-x86_64d.a -lcurl  -lstdc++ -ldl"
+    else
+        ngx_feature_libs="-L/home/rc15345/code/CQPToolkit/build/gcc/src/CQPToolkit -l:libCQPToolkit-x86_64d.a -lcurl  -lstdc++ -ldl"
+    fi
+
+    . auto/feature
+fi
+
+
+if [ $ngx_found = yes ]; then
+
+    CORE_INCS="$CORE_INCS $ngx_feature_path"
+
+    if [ $HTTP_SSL_PSK = YES ]; then
+        CORE_LIBS="$CORE_LIBS $ngx_feature_libs"
+    fi
